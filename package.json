{
  "name": "analysis-wrapper",
  "version": "2.0.0",
  "description": "Opsramp Analysis Wrapper",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@coreui/coreui": "^3.2.2",
    "@coreui/icons": "2.0.0-beta.4",
    "@coreui/icons-react": "^1.0.1",
    "@coreui/react": "^3.0.3",
    "@coreui/utils": "^1.3.1",
    "@primer/octicons-react": "^9.4.0",
    "core-js": "^3.6.5",
    "formik": "^2.2.9",
    "match-sorter": "^6.3.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "node-sass": "^4.14.1",
    "opsramp-design-system": "^0.4.14",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-dom": "^16.13.1",
    "react-moment": "^1.1.1",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.0",
    "react-toastify": "^7.0.4",
    "rewire": "^5.0.0",
    "source-map-explorer": "^2.5.0",
    "yup": "^0.32.9"
  },
  "resolutions": { "coa": "2.0.2" },
  "devDependencies": {
    "react-scripts": "^3.4.3"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "node ./scripts/build.js",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|css|md)\"",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/*index.js",
      "!src/serviceWorker.js",
      "!src/polyfill.js"
    ]
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  }
}
